    $( document ).ready(function() {

        var quoteSelection = randomIntFromInterval(1,2) ;
        console.log(quoteSelection);

        if(quoteSelection == 1){
            $('.quote2').hide();
            $('.quote1').show();
            $('.quote10').hide();
            $('.quote11').show();
        }
        else{
            $('.quote1').hide();
            $('.quote2').show();
            $('.quote10').show();
            $('.quote11').hide();
        }



    });

    function randomIntFromInterval(min,max)
    {
        return Math.floor(Math.random()*(max-min+1)+min);
    }

    $(window).load(function() {
